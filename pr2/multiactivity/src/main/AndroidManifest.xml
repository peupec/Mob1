<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mirea.utukina.multiactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Rumireakrotovpractice2">
        <activity
            android:name="ru.mirea.utukina.multiactivity.SecondActivity"
            android:label="@string/title_activity_second"
            android:theme="@style/Theme.Design"></activity>
        <activity android:name="ru.mirea.utukina.multiactivity.MultiActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>